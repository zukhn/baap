ip: 192.168.10.10
memory: 1024
cpus: 2
provider: virtualbox
authorize: ~/.ssh/id_rsa.pub
keys:
    - ~/.ssh/id_rsa
folders:
    -
        map: /baap
        to: /home/vagrant/code
sites:
    -
        map: baap.test
        to: /home/vagrant/code/public
databases:
    - baap
features:
    -
        mariadb: false
    -
        ohmyzsh: false
    -
        webdriver: false
name: baap
hostname: baap
